#!/usr/bin/perl

use 5.010;
use strict;
use warnings;

use Perinci::CmdLine;

our $VERSION = '0.01'; # VERSION

Perinci::CmdLine->new(url=>'/Business/ID/Mandiri/list_mandiri_branches')->run;

1;
# ABSTRACT: List Mandiri branches
# PODNAME: list-mandiri-branches


__END__
=pod

=encoding utf-8

